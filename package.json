{
    "name": "cdk-cognito-userpool",
    "version": "0.1.0",
    "bin": {
        "cdk-cognito-userpool": "bin/cdk-cognito-userpool.js"
    },
    "scripts": {
        "lint": "npx eslint **/*.ts --fix --quiet",
        "build": "npx rimraf dist & tsc",
        "build-w": "npx rimraf dist & webpack",
        "watch": "tsc -w",
        "test": "jest --passWithNoTests",
        "diff-dev": "npx cdk diff cdk-cognito-userpool-dev --ci --require-approval never --no-asset-metadata",
        "diff-tst": "npx cdk diff cdk-cognito-userpool-tst --ci --require-approval never --no-asset-metadata",
        "diff-prd": "npx cdk diff cdk-cognito-userpool-prd --ci --require-approval never --no-asset-metadata",
        "deploy-dev": "npx cdk deploy cdk-cognito-userpool-dev --ci --require-approval never --no-asset-metadata",
        "deploy-tst": "npx cdk deploy cdk-cognito-userpool-tst --ci --require-approval never --no-asset-metadata",
        "deploy-prd": "npx cdk deploy cdk-cognito-userpool-prd --ci --require-approval never --no-asset-metadata"
    },
    "devDependencies": {
        "@types/jest": "^27.5.1",
        "@types/node": "^17.0.35",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "aws-cdk": "^2.25.0",
        "aws-cdk-lib": "^2.25.0",
        "constructs": "^10.1.15",
        "eslint": "^8.16.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-jest": "^26.2.2",
        "jest": "^28.1.0",
        "jest-sonar": "^0.2.12",
        "ts-jest": "^28.0.2",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.0",
        "typescript": "~4.6.4",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "source-map-support": "^0.5.21"
    }
}
